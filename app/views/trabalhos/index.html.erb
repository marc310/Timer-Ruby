<p id="notice"><%= notice %></p>

<h1>Trabalhos</h1>

<table>
  <thead>
    <tr>
      <th>Inicio</th>
      <th>Final</th>
      <th>Horas</th>
      <th>Rendimento</th>
      <th>Livre</th>
      <th>Faturado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trabalhos.each do |trabalho| %>
      <tr>
        <td><%= trabalho.inicio %></td>
        <td><%= trabalho.final %></td>
        <td><%= trabalho.horas %></td>
        <td><%= trabalho.rendimento %></td>
        <td><%= trabalho.livre %></td>
        <td><%= trabalho.faturado %></td>
        <td><%= link_to 'Show', trabalho %></td>
        <td><%= link_to 'Edit', edit_trabalho_path(trabalho) %></td>
        <td><%= link_to 'Destroy', trabalho, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trabalho', new_trabalho_path %>
